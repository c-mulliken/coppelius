{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://coppelius-production.up.railway.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ]
}
